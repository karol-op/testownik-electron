{
  "name": "testownik-electron",
  "productName": "Testownik",
  "version": "0.7.0",
  "author": "Kamil Golec <kumalgfilms@gmail.com>",
  "description": "An electron-vue project",
  "license": null,
  "main": "./dist/electron/main.js",
  "repository": "https://github.com/kumalg/testownik-electron",
  "scripts": {
    "start": "electron . --allow-file-access-from-files",
    "build": "electron-builder",
    "build:dir": "electron-builder --dir",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue src test",
    "lint:fix": "eslint --ext .js,.vue --fix src test",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Testownik",
    "appId": "org.testownik.app",
    "publish": ["github"],
    "directories": {
      "output": "build"
    },
    "files": ["dist/electron/**/*"],
    "win": {
      "target": ["nsis", "portable"],
      "icon": "build/icons/icon.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "axios": "^1.6.0",
    "electron-settings": "^4.0.2",
    "electron-updater": "^5.3.0",
    "moment": "^2.30.1",
    "sass": "^1.64.2",
    "sass-loader": "^12.6.0",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2",
    "windows-1250": "^1.0.0"
  },
  "devDependencies": {
    "electron": "^26.2.0",
    "electron-builder": "^24.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.53.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^7.1.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0"
  }
}
